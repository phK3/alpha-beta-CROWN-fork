(assert (>= X_7 0.0))
(assert (<= X_7 1.0))
(assert (>= X_8 0.0))
(assert (<= X_8 1.0))
(assert (>= X_9 0.0))
(assert (<= X_9 1.0))
(assert (>= X_10 0.0))
(assert (<= X_10 1.0))
(assert (>= X_11 0.0))
(assert (<= X_11 1.0))
(assert (>= X_12 0.0))
